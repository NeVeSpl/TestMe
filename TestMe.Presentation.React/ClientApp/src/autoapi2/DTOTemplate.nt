{{- end }}

    //eslint-disable-next-line
    constructor()
    {
    {{  if dto.HasBaseClass }}    super(); {{-end }}
    {{- for prop in dto.Properties }}
        this.{{ prop.Name | String.ToCamelCase }} = {{ prop.Type | Type.ToTypeScriptDefault }};
    {{- end }}
    }
}
